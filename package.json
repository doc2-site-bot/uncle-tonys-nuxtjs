    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "hast-to-hyperscript": "^10.0.1",
    "hast-util-select": "^5.0.3",
    "hast-util-to-text": "^3.1.2",
    "hastscript": "^7.2.0",
    "nuxt": "3.0.0",
    "postcss": "^8.4.21",
